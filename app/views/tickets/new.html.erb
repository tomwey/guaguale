<div class="page-header">
	<h1>添加新奖券</h1>
</div>
<div class="row">
	<div class="span6 offset3">
		<%= form_for(@ticket, :html => { :multipart => true }) do |f| %>
			<%= render 'fields', :f => f %>
			<br />
		  <%= f.submit "添加一个新奖券", class: "btn btn-primary" %>
			<%= link_to '返回', tickets_path, class: "btn" %>

		<% end %>
	</div>
</div>
		
