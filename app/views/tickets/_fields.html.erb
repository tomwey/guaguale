<%= f.error_messages %>
<%= f.label :expire_start_at, "有效期开始日" %>
<%= f.text_field 'expire_start_at', value: "#{Date.today}", class:"datepicker" %>

<%= f.label :expire_end_at, "有效期截止日"%>
<%= f.text_field 'expire_end_at',value: "#{Date.tomorrow}", class:"datepicker" %>

